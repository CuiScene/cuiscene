<h1>My Food Log</h1>

  <div class="user_log"> 
    
    {{#each user_entries}}
      <div class="card">
        
        <div class="card-header">  
          <h3>{{this.restaurant_name_pk}}</h3>
        </div>
          
        <div class="card-body"> 
          <p>Ordered {{this.menu_item}} on {{this.date}} for {{this.meal_time}} | My Rating: {{this.rating}}</p>
          <p>Notes about {{this.menu_item}}: {{this.notes}}</p>
        </div>

      </div>

    {{/each}}

  </div>