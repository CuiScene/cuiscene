{{#each user_entries}}
  <div class="user_log"> 
    <div class="card">
      <div class="card-header">  
        <h3>{{restaurant_name_pk}}</h3>
      </div>
        
      <div class="card-body"> 
        <p>Ordered {{menu_item}} on {{this.date}} for {{this.meal_time}} | My Rating: {{this.rating}}</p>
        <p>Item Notes: {{this.notes}}</p>
      </div>
    </div>
  </div>
{{/each}}
